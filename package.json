{
  "name": "my-projects",
  "source": "src/assets/js/index.js",
  "scripts": {
    "build": "parcel build ./src/*.html --public-url ./ --no-source-maps",
    "dev": "parcel ./src/*.html",
    "dev-public": "parcel ./src/*.html --host 0.0.0.0",
    "format": "npx prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "cssnano": "^5.1.12",
    "husky": "^7.0.4",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sass": "^1.32.12"
  },
  "dependencies": {
    "primeflex": "^3.1.3",
    "random-words": "^1.1.2"
  }
}
